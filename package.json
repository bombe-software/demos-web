{
  "name": "demos-web",
  "version": "0.1.0",
  "private": true,
  "main": "./public/electron.js",
  "scripts": {
    "start": "node index.js",
    "dev": "set PORT=9000 && react-scripts start",
    "build": "react-scripts build",
    "dev-pc": "electron .",
    "build-pc": "react-scripts build && electron-builder .",
    "eject": "react-scripts eject"
