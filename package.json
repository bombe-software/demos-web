  "dependencies": {
    "@google/maps": "^0.4.6",
    "@material-ui/core": "^1.2.0",
    "apollo-boost": "^0.1.7",
    "apollo-link-ws": "^1.0.8",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.7.2",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "final-form": "^4.8.0",
    "graphql": "^14.0.0-rc.1",
    "graphql-tag": "^2.9.2",
    "install": "^0.11.0",
    "lodash": "^4.17.10",
    "material-ui": "^0.20.1",
    "material-ui-password-field": "^2.0.1",
    "npm": "^6.1.0",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-async-script": "^0.9.1",
    "react-chartjs-2": "^2.7.2",
